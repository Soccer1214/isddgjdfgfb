-- Define the function to create a toggle
local function CreateToggle(Name, CurrentValue, Callback)
    local Toggle = Tab2:CreateToggle({
        Name = Name,
        CurrentValue = CurrentValue,
        Flag = "Toggle1",
        Callback = Callback,
    })
    return Toggle
end

-- Create the toggle
